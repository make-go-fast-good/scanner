{
  "author": "pcrandall",
  "name": "plc_toolkit",
  "version": "1.0.0",
  "private": true,
  "homepage": "./",
  "main": "./public/electron.js",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@phuocng/react-pdf-viewer": "^1.7.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.0",
    "@types/react-pdf": "^5.0.1",
    "axios": "^0.21.1",
    "concurrently": "^6.0.1",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "g": "^2.0.1",
    "material-icons": "^0.5.3",
    "material-table": "^1.69.2",
    "mui-datatables": "^3.7.6",
    "nodes7": "^0.3.12",
    "opn": "^6.0.0",
    "pdfjs-dist": "^2.4.456",
    "pkg": "^4.4.9",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-data-grid": "^7.0.0-canary.36",
    "react-dom": "^17.0.1",
    "react-html-parser": "^2.0.2",
    "react-iframe": "^1.8.0",
    "react-modal": "^3.12.1",
    "react-modal-image": "^2.5.0",
    "react-pdf": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-simple-code-editor": "^0.11.0",
    "react-spinners": "^0.10.6",
    "react-switch": "^6.0.0",
    "serve": "^11.3.2",
    "styled-components": "^5.2.1",
    "typescript": "^4.2.2",
    "wait-on": "^5.3.0"
  },
  "scripts": {
    "react-start": "concurrently \"react-scripts start\" \"cd ./serve && node server.js \"",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder --win portable",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\" \"cd ./serve && node server.js \"",
    "electron-pack": "build --em.main=build/electron.js",
    "preelectron-pack": "yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@types/prismjs": "^1.16.3",
    "@types/react-dom": "^17.0.1",
    "electron-builder": "^22.10.5",
    "electron": "^12.0.2"
  },
  "build": {
    "productName": "PLC Toolkit",
    "appId": "com.plc_toolkit.electron-cra",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "installerIcon": "build/icon.ico",
      "installerHeaderIcon": "build/icon.ico",
      "deleteAppDataOnUninstall": true
    }
  }
}
