{
  "package": {
    "productName": "PLC Toolkit",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../build",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "false",
      "identifier": "com.tauri.phillip_crandall",
      "icon": ["icons/icon.ico"],
      "resources": [],
      "externalBin": ["s7_Server/s7_Server"],
      "copyright": "",
      "category": "Productivity",
      "shortDescription": "Read Data from Siemens S7 PLCs",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": false
    },
    "windows": [
      {
        "title": "PLC Toolkit",
        "resizable": true,
        "fullscreen": false,
        "minWidth": 1250,
        "minHeight": 600,
        "maximized": true
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    }
  }
}
